<%- include("../../partials/header") %>

<div class="col-12">
    <div class="card">
        <div class="card-header">
            <h4><%= title%></h4>
            <div class="card-header-action d-flex ">
                <button type="button"
                        class="btn btn-primary"
                        id="location-modal"
                        data-id="0"
                        data-toggle="modal"
                        data-target="#fire-modal-location"
                ><i class="fas fa-plus"></i></button>
            </div>
        </div>
        <div class="card-body p-0">
            <div class="col-md-12">
                <div class="table-responsive">
                    <table class="table table-striped" id="locationTables" style="width:100%">
                        <thead>
                        <tr>
                            <th>Name</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include("LocationForm") %>
<%- include("../../partials/footer") %>
